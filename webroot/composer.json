{
  "name": "gpalab/content-webroot",
  "description": "Webroot for the Content WordPress site used to upload articles into Content Commons",
  "repositories": [
    {
      "type": "package",
      "package": {
        "name": "wordpress/wordpress",
        "type": "webroot",
        "version": "5.6.2",
        "dist": {
          "type": "zip",
          "url": "https://github.com/WordPress/WordPress/archive/5.6.2.zip"
        },
        "require": {
          "fancyguy/webroot-installer": "1.1.0"
        }
      }
    },
    {
      "type": "composer",
      "url": "https://wpackagist.org"
    },
    {
      "type": "git",
      "url": "git@github.com:IIP-Design/wp-hide-login-form.git"
    },
    {
      "type": "git",
      "url": "git@github.com:IIP-Design/wp-elasticsearch-feeder.git"
    },
    {
      "type": "git",
      "url": "git@github.com:humanmade/S3-Uploads.git"
    }
  ],
  "require": {
    "wordpress/wordpress": "5.6.2",
    "fancyguy/webroot-installer": "1.1.0",
    "humanmade/s3-uploads": "2.2.2",
    "wpackagist-theme/twentynineteen":"*",
    "iip-design/wp-hide-login-form": "v1.1.0",
    "wpackagist-plugin/google-apps-login": "3.4.3",
    "wpackagist-plugin/wp-ses":"1.4.4",
    "iip-design/wp-elasticsearch-feeder": "2.5.0"
  },
  "extra": {
    "webroot-dir": "wp",
    "webroot-package": "wordpress/wordpress"
  }
}
